	<queryString>
		<![CDATA[SELECT
            albaranescli.codigo as codigoalbaran,
            albaranescli.codejercicio as ejercicioalbaran,
            albaranescli.codserie as seriealbaran,
            albaranescli.numero as numeroalbaran,
            albaranescli.fecha as fechaalbaran,
            albaranescli.coddivisa as divisaalbaran,
            albaranescli.codalmacen as almacenalbaran,
            albaranescli.codcliente as codcliente,
            albaranescli.nombrecliente as nombrecliente,
            albaranescli.cifnif as cifnifcliente,
            albaranescli.direccion as direccioncliente,
            albaranescli.codpostal as cpcliente,
            albaranescli.ciudad as ciudadcliente,
            albaranescli.provincia as provinciacliente,
            albaranescli.apartado as apartadocliente,
            paises.nombre as paiscliente,
            albaranescli.codpedido as codpedcliente,
            albaranescli.nombrebuque as nombrebuque,
            albaranescli.codmotor as motorbuque,
            albaranescli.observaciones as observacionesalbaran,
            albaranescli.neto as baseimponiblealbaran,
            albaranescli.regimeniva as regimenivaalbaran,
            albaranescli.totaliva as totalivaalbaran,
            albaranescli.total as totalalbaran,
            formaspago.descripcion as formapago,
            albaranescli.entrega as entrega_paqueteria,
            albaranescli.direntrega as direntrega_paqueteria,
            albaranescli.fechalimite as fechalimite_paqueteria,
            albaranescli.agencia as agencia_paqueteria,
            albaranescli.dimensiones as dimensiones_paqueteria,
            albaranescli.tipoportes as tipoportes_paqueteria,
            albaranescli.impportes as impportes_paqueteria,
            albaranescli.gestaduana as gestaduana_paqueteria,
            pedidoscli.codigo as codigopedido,
            pedidoscli.fecha as fechapedido,
            lineasalbaranescli.idpedido as idpedidolinea,
            lineasalbaranescli.idalbaran as idalbaranlinea,
            lineasalbaranescli.numlinea as numlinea,
            lineasalbaranescli.cantidad as cantidadlinea,
            lineasalbaranescli.referencia as referencialinea,
            lineasalbaranescli.descripcion as descripcionlinea,
            lineasalbaranescli.pvpunitario as unitariolinea,
            lineasalbaranescli.pvptotal as pvptotallinea,
            lineasalbaranescli.pvpsindto as pvpsindtolinea,
            lineasalbaranescli.iva as porivalinea,
            empresa.nombre as nombreempresa,
            empresa.cifnif as cifempresa,
            empresa.direccion as direccionempresa,
            empresa.codpostal as cpempresa,
            empresa.ciudad as ciudadempresa,
            empresa.provincia as provinciaempresa
            FROM empresa, albaranescli
            INNER JOIN lineasalbaranescli ON albaranescli.idalbaran = lineasalbaranescli.idalbaran
            INNER JOIN formaspago ON albaranescli.codpago = formaspago.codpago
            INNER JOIN clientes ON clientes.codcliente = albaranescli.codcliente
            INNER JOIN paises ON paises.codpais = albaranescli.codpais
            LEFT OUTER JOIN pedidoscli ON pedidoscli.idpedido = lineasalbaranescli.idpedido
            WHERE $P!{WHERE}
            ORDER BY $P!{ORDERBY}]]>
	</queryString>
