            facturascli.codejercicio as ejerciciofactura,
            facturascli.codserie as seriefactura,
            facturascli.numero as numerofactura,
            facturascli.fecha as fechafactura,
            facturascli.coddivisa as divisafactura,
            facturascli.codalmacen as almacenfactura,
            facturascli.codcliente as codcliente,
            facturascli.nombrecliente as nombrecliente,
            facturascli.cifnif as cifnifcliente,
            facturascli.direccion as direccioncliente,
            facturascli.codpostal as cpcliente,
            facturascli.ciudad as ciudadcliente,
            facturascli.provincia as provinciacliente,
            facturascli.apartado as apartadocliente,
            paises.nombre as paiscliente,
            facturascli.codpedido as codpedcliente,
            facturascli.nombrebuque as nombrebuque,
            facturascli.codmotor as motorbuque,
            facturascli.observaciones as observacionesfactura,
            facturascli.neto as baseimponiblefactura,
            facturascli.regimeniva as regimenivafactura,
            facturascli.totaliva as totalivafactura,
            facturascli.total as totalfactura,
            facturascli.totalmanoobra as totalmanoobraservicio,
            facturascli.totaldietas as totaldietasservicio,
            facturascli.totalexternos as totalexternosservicio,
            facturascli.totalmatlim as totalmatlimservicio,
            facturascli.totalivalim as totalivalimservicio,
            facturascli.solucion as trabajosservicio,
            facturascli.codpago as codpago,
            formaspago.descripcion as formapago,
            cuentasbcocli.ctaentidad as ctaentidadcliente,
            cuentasbcocli.ctaagencia as ctaagenciacliente,
            cuentasbcocli.cuenta as ctabancocliente,
            bancos.nombre as nombrebancocliente,
            facturascli.entrega as entrega_paqueteria,
            facturascli.direntrega as direntrega_paqueteria,
            facturascli.fechalimite as fechalimite_paqueteria,
            facturascli.agencia as agencia_paqueteria,
            facturascli.dimensiones as dimensiones_paqueteria,
            facturascli.tipoportes as tipoportes_paqueteria,
            facturascli.impportes as impportes_paqueteria,
            facturascli.gestaduana as gestaduana_paqueteria,
            albaranescli.codigo as codigoalbaran,
            albaranescli.fecha as fechaalbaran,
            servicioscli.numservicio as numeroservicio,
            servicioscli.fecha as fechaservicio,
            lineasfacturascli.idalbaran as idalbaranlinea,
            lineasfacturascli.idservicio as idserviciolinea,
            lineasfacturascli.idfactura as idfacturalinea,
            lineasfacturascli.numlinea as numlinea,
            lineasfacturascli.cantidad as cantidadlinea,
            lineasfacturascli.referencia as referencialinea,
            lineasfacturascli.descripcion as descripcionlinea,
            lineasfacturascli.pvpunitario as unitariolinea,
            lineasfacturascli.pvptotal as pvptotallinea,
            lineasfacturascli.pvpsindto as pvpsindtolinea,
            lineasfacturascli.iva as porivalinea,
            empresa.nombre as nombreempresa,
            empresa.cifnif as cifempresa,
            empresa.direccion as direccionempresa,
            empresa.codpostal as cpempresa,
            empresa.ciudad as ciudadempresa,
            empresa.provincia as provinciaempresa
            FROM empresa, facturascli
            LEFT JOIN lineasfacturascli ON facturascli.idfactura = lineasfacturascli.idfactura
            LEFT JOIN formaspago ON facturascli.codpago = formaspago.codpago
            LEFT JOIN clientes ON clientes.codcliente = facturascli.codcliente
            LEFT JOIN paises ON paises.codpais = facturascli.codpais
            LEFT JOIN cuentasbcocli ON cuentasbcocli.codcliente = facturascli.codcliente
            LEFT OUTER JOIN bancos ON bancos.entidad = cuentasbcocli.ctaentidad
            LEFT OUTER JOIN albaranescli ON albaranescli.idalbaran = lineasfacturascli.idalbaran
            LEFT OUTER JOIN servicioscli ON servicioscli.idservicio = lineasfacturascli.idservicio
            WHERE $P!{WHERE}
            ORDER BY $P!{ORDERBY}]]>
